                    dashboardData = data;
                    $('#TotalPositions').html(data.TotalPositions.HeadCount);
                    $('#ActivePositions').html(data.ActivePositions.HeadCount);
                    $('#ClosedPositions').html(data.ClosedPositions.HeadCount);
                    $('#ResumeUploaded').html(data.ResumeUploaded);
                    showTotalPositions();
                }

            },
            error: function (err) {
                console.log(err);
            }
        });
    });
</script>