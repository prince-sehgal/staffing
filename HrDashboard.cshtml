    function showTotalPositions() {
        $('[id="position-type"]').each(function () {
            $(this).html('Total Positions');
        });
        $('#recruiter-list').html('');
        var htmlContent = '';
        dashboardData.TotalPositions.Spoc.forEach(function (item) {
            htmlContent += '<a href="javascript:void(0);" onclick="refreshCharts(\'' + item.Name + '\');"><div class="r-circle"><div class="recuriter-circle"><div class="inner" style="text-align: center; padding: 4px;"><h3>' + item.HeadCount + '</h3></div></div><div class="recuriter-title">' + item.Name + '</div></div></a>';
        });
        $('#recruiter-list').html(htmlContent);
        $('#pieChartLOB').remove(); // this is my <canvas> element
        $('#lob-graph-container').append('<canvas id="pieChartLOB" height="150" width="110"></canvas>');
